from Sites a
left join (select * 
from (select ROW_NUMBER() OVER (PARTITION BY a.DistrictID,a.SiteID,b.SiteName order by a.DistrictID,a.SiteID,b.SiteName,a.EndDate,DateClosed) as RowNumber,a.SiteID,b.DistrictID, c.FirstName,c.LastName,a.SiteEmployeeID,a.EmployeeID
