case when Coordinates.EnvelopeAngle() &gt;= 90 then Coordinates.ReorientObject().EnvelopeCenter().Long else Coordinates.EnvelopeCenter().Long end as Longitude,
case when Coordinates.EnvelopeAngle() &gt;= 90 then 
Coordinates.ReorientObject().EnvelopeCenter().Lat else 
