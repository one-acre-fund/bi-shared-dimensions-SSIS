                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select a.DistrictID as RosterDistrictID, ISNULL(SiteName,-1) as SiteName,ISNULL(SectorID,-1) as  RosterSectorID, ISNULL(FirstSeason,-1) as FirstSeason, ISNULL(LastSeason,-1) as LastSeason, 
